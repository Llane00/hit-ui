  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Llane00/hit-ui.git"
  },
  "keywords": [
    "react",
    "ui"
  ],
  "author": "Llane00",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Llane00/hit-ui/issues"
  },
  "homepage": "https://github.com/Llane00/hit-ui#readme",
  "dependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@types/enzyme": "^3.10.9",
    "@types/jest": "^24.0.9",
    "@types/react": "^16.8.4",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.1",
